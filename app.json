{
  "expo": {
    "name": "spark-msu",
    "slug": "spark-msu",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "sparkmsu",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "We use your location to verify lot reports near where you are."
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": ["ACCESS_FINE_LOCATION"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2E5A35"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/icon.png"
    },
    "icon": "./assets/icon.png",
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#2E5A35"
      
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}

